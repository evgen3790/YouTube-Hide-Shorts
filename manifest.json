{
  "manifest_version": 3,
  "name": "YouTube Shorts Hider",
  "version": "1.0",
  "description": "Hides the Shorts menu item on YouTube",
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "css": ["styles.css"],
      "js": ["content.js"]
    }
  ],
  "permissions": []
}